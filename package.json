{
  "author": "Robert Smieja",
  "dependencies": {
    "@types/ramda": "^0.27.19",
    "@types/ws": "^7.2.6",
    "discord.js": "^12.3.1",
    "immer": "^7.0.9",
    "ramda": "^0.27.1"
  },
  "description": "Core of RobBot, a Discord bot",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12",
    "pnpm": ">=5"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "keywords": [
    "discord",
    "discordjs",
    "robbot",
    "robbot-core",
    "robbot-discord"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ],
    "src/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "@robbot/robbot-core",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "repository": "https://github.com/robbot-discord/robbot-core",
  "scripts": {
    "build": "npm-run-all --parallel build:js lint",
    "build:js": "tsc",
    "clean": "rimraf dist/",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint --fix ./src/**/*",
    "lint:staged": "lint-staged",
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "npm-run-all --serial clean build --parallel test lint",
    "prestart": "npm run-script lint",
    "prettier": "prettier --write \"./**/*.{js,ts,jsx,tsx,json,md}\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "version": "npm run lint:fix && git add -A src"
  },
  "version": "0.0.12"
}
